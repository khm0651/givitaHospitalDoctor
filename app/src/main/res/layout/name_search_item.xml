<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:id="@+id/linear_head"
        android:layout_width="980dp"
        android:layout_height="44dp"
        android:background="@drawable/rv_name_search_item_solid"
        android:orientation="horizontal"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/tv_nSearch_id"
            android:layout_width="33dp"
            android:layout_height="15dp"
            android:layout_gravity="center"
            android:layout_marginLeft="30dp"
            android:fontFamily="@font/spoqa_han_sans_neo_medium"
            android:text="36780"
            android:textColor="#212121"
            android:textSize="10dp" />

        <TextView
            android:id="@+id/tv_nSearch_createdDate"
            android:layout_width="50dp"
            android:layout_height="15dp"
            android:layout_gravity="center"
            android:layout_marginLeft="24dp"
            android:fontFamily="@font/spoqa_han_sans_neo_medium"
            android:text="20.09.04"
            android:textColor="#212121"
            android:textSize="10dp" />

        <TextView
            android:id="@+id/tv_nSearch_name"
            android:layout_width="33dp"
            android:layout_height="15dp"
            android:layout_gravity="center"
            android:layout_marginLeft="25dp"
            android:fontFamily="@font/spoqa_han_sans_neo_medium"
            android:text="조윤진"
            android:textColor="#212121"
            android:textSize="10dp" />

        <TextView
            android:id="@+id/tv_nSearch_sex"
            android:layout_width="11dp"
            android:layout_height="15dp"
            android:layout_gravity="center"
            android:layout_marginLeft="33dp"
            android:fontFamily="@font/spoqa_han_sans_neo_medium"
            android:text="여"
            android:textColor="#212121"
            android:textSize="10dp" />

        <TextView
            android:id="@+id/tv_nSearch_age"
            android:layout_width="12dp"
            android:layout_height="15dp"
            android:layout_gravity="center"
            android:layout_marginLeft="40dp"
            android:fontFamily="@font/spoqa_han_sans_neo_medium"
            android:text="51"
            android:textColor="#212121"
            android:textSize="10dp" />

        <TextView
            android:id="@+id/tv_nSearch_disease"
            android:layout_width="20dp"
            android:layout_height="15dp"
            android:layout_gravity="center"
            android:layout_marginLeft="43dp"
            android:fontFamily="@font/spoqa_han_sans_neo_medium"
            android:text="2형"
            android:textColor="#212121"
            android:textSize="10dp" />

        <LinearLayout
            android:layout_width="35dp"
            android:layout_height="25dp"
            android:layout_marginLeft="36dp"
            android:layout_marginTop="15dp"
            android:orientation="vertical">

            <TextView
                android:id="@+id/tv_nSearch_bp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:fontFamily="@font/spoqa_han_sans_neo_medium"
                android:text="141/70"
                android:textColor="#212121"
                android:textSize="10dp" />

            <TextView
                android:id="@+id/tv_nSearch_bpDate"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:fontFamily="@font/spoqa_han_sans_neo_medium"
                android:text="210621"
                android:textColor="#212121"
                android:textSize="7dp" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="35dp"
            android:layout_height="25dp"
            android:layout_marginLeft="29dp"
            android:layout_marginTop="15dp"
            android:orientation="vertical">
            <TextView
                android:id="@+id/tv_nSearch_bs"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:fontFamily="@font/spoqa_han_sans_neo_medium"
                android:text="101"
                android:textColor="#212121"
                android:textSize="10dp" />

            <TextView
                android:id="@+id/tv_nSearch_bsDate"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:fontFamily="@font/spoqa_han_sans_neo_medium"
                android:text="210621"
                android:textColor="#212121"
                android:textSize="7dp" />
        </LinearLayout>

        <TextView
            android:id="@+id/tv_nSearch_lastvisitDate"
            android:layout_width="47dp"
            android:layout_height="15dp"
            android:layout_gravity="center"
            android:layout_marginLeft="35dp"
            android:fontFamily="@font/spoqa_han_sans_neo_medium"
            android:gravity="center"
            android:text="21.06.19"
            android:textColor="#212121"
            android:textSize="10dp" />

        <TextView
            android:id="@+id/tv_nSearch_request"
            android:layout_width="47dp"
            android:layout_height="15dp"
            android:layout_gravity="center"
            android:layout_marginLeft="24.5dp"
            android:fontFamily="@font/spoqa_han_sans_neo_medium"
            android:gravity="center"
            android:text="21.06.19"
            android:textColor="#212121"
            android:textSize="10dp" />


        <LinearLayout
            android:layout_width="35dp"
            android:layout_height="25dp"
            android:layout_marginLeft="21dp"
            android:layout_marginTop="15dp"
            android:orientation="vertical">
            <TextView
                android:id="@+id/tv_nSearch_receive"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:fontFamily="@font/spoqa_han_sans_neo_medium"
                android:text="Y"
                android:textColor="#212121"
                android:textSize="10dp" />

            <TextView
                android:id="@+id/tv_nSearch_receiveDate"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:fontFamily="@font/spoqa_han_sans_neo_medium"
                android:text="210622"
                android:textColor="#212121"
                android:textSize="7dp" />

        </LinearLayout>
        <TextView
            android:id="@+id/btn_name_search_education"
            android:layout_width="80.5dp"
            android:layout_height="23.5dp"
            android:layout_marginLeft="15.5dp"
            android:layout_marginTop="11.5dp"
            android:background="@color/btnGray"
            android:text="3차"
            android:textSize="10dp"
            android:gravity="center"
            android:textColor="#ffffff"
            android:fontFamily="@font/spoqa_han_sans_neo_medium"/>
        <TextView
            android:id="@+id/btn_name_search_emr"
            android:layout_width="80.5dp"
            android:layout_height="23.5dp"
            android:layout_marginLeft="4dp"
            android:layout_marginTop="11.5dp"
            android:background="@color/btnGray"
            android:text="EMR에 등록"
            android:textSize="10dp"
            android:gravity="center"
            android:textColor="#ffffff"
            android:fontFamily="@font/spoqa_han_sans_neo_medium"/>
        <TextView
            android:id="@+id/btn_name_search_receive"
            android:layout_width="80.5dp"
            android:layout_height="23.5dp"
            android:layout_marginLeft="4dp"
            android:layout_marginTop="11.5dp"
            android:background="@color/btnGray"
            android:text="측정 요청"
            android:textSize="10dp"
            android:gravity="center"
            android:textColor="#ffffff"
            android:fontFamily="@font/spoqa_han_sans_neo_medium"/>
</LinearLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="980dp"
        android:layout_height="29dp"
        android:layout_marginTop="3.5dp"
        android:background="@drawable/linearlayout_name_search_solid"
        android:orientation="horizontal"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@id/linear_head">
        <ImageView
            android:id="@+id/iv_visit_request"
            android:layout_width="12dp"
            android:layout_height="12dp"
            android:src="@drawable/ic_calendar_02"
            android:layout_marginRight="5dp"
            app:layout_constraintRight_toLeftOf="@id/tv_visit_request"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
          />
        <TextView
            android:id="@+id/tv_visit_request"
            android:layout_width="49dp"
            android:layout_height="17.5dp"
            android:layout_gravity="center"
            android:text="내원 요청"
            android:layout_marginRight="28.5dp"
            android:fontFamily="@font/spoqa_han_sans_neo_medium"
            android:textColor="#4B4B4B"
            android:textSize="12.5dp"
            app:layout_constraintRight_toLeftOf="@id/ic_movement_recommad"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"/>
        <View
            android:layout_width="0.5dp"
            android:layout_height="14.5dp"
            android:background="#000000"
            android:layout_marginRight="16dp"
            app:layout_constraintRight_toLeftOf="@id/ic_movement_recommad"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"/>
        <ImageView
            android:id="@+id/ic_movement_recommad"
            android:layout_width="12dp"
            android:layout_height="12dp"
            android:src="@drawable/ic_calendar_3"
            android:layout_marginRight="5dp"
            app:layout_constraintRight_toLeftOf="@id/tv_movement_recommad"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"/>
        <TextView
            android:id="@+id/tv_movement_recommad"
            android:layout_width="97dp"
            android:layout_height="17.5dp"
            android:text="운동 / 식이 추천"
            android:fontFamily="@font/spoqa_han_sans_neo_medium"
            android:textSize="12.5dp"
            android:textColor="#4B4B4B"
            android:layout_marginRight="10.5dp"
            app:layout_constraintRight_toLeftOf="@id/iv_call_request"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"/>
        <View
            android:layout_width="0.5dp"
            android:layout_height="14.5dp"
            android:background="#000000"
            android:layout_marginRight="26.5dp"
            app:layout_constraintRight_toLeftOf="@id/tv_call_request"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"/>
        <ImageView
            android:id="@+id/iv_call_request"
            android:layout_width="12dp"
            android:layout_height="12dp"
            android:src="@drawable/ic_phone"
            android:layout_marginRight="5dp"
            app:layout_constraintRight_toLeftOf="@id/tv_call_request"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"/>
        <TextView
            android:id="@+id/tv_call_request"
            android:layout_width="81dp"
            android:layout_height="17.5dp"
            android:text="전화 상담 요청"
            android:fontFamily="@font/spoqa_han_sans_neo_medium"
            android:textSize="12.5dp"
            android:textColor="#4B4B4B"
            android:layout_marginRight="13.5dp"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>